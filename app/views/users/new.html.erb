<h1>Users#new</h1>

<%= form_for @user do |t| %>
<table>
	<tr><td> Username  </td>
	<td><%= t.text_field :username %></td></tr>
	<tr><td> Password </td>
	<td><%= t.password_field :password %></td></tr>
	<tr><td> Email </td>
	<td> <%= t.text_field :email %></td></tr>
	<tr><td></td><td><%= t.submit %></td></tr>
</table>
<% end %>
